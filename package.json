{
	"name": "spomoda",
	"version": "1.0.0",
	"description": "spomoda",
	"wolkenkit": {
		"application": "spomoda",
		"runtime": {
			"version": "3.0.0"
		},
		"environments": {
			"default": {
				"api": {
					"address": {
						"host": "local.wolkenkit.io",
						"port": 3000
					},
					"allowAccessFrom": "*"
				},
				"fileStorage": {
					"allowAccessFrom": "*"
				},
				"identityProvider": {
					"name": "https://gossi.eu.auth0.com/",
					"certificate": "/server/keys/auth0"
				},
				"node": {
					"environment": "development"
				}
			}
		}
	},
	"dependencies": {
		"slugify": "^1.3.4",
		"wolkenkit-command-tools": "^3.0.0"
	},
	"keywords": [
		"spomoda"
	],
	"license": "MIT"
}
